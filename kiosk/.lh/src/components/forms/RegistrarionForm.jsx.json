            "content": "import React from 'react';\r\n\r\nconst Signup = () => {\r\n  return (\r\n    <div className=\"min-h-screen flex items-center justify-center bg-gray-100 p-6\">\r\n      <div className=\"bg-white shadow-lg rounded-lg w-full max-w-4xl flex flex-col md:flex-row\">\r\n        {/* Left Image */}\r\n        <div className=\"w-full md:w-1/2\">\r\n          <img\r\n            src=\"rectangle-170.png\"\r\n            alt=\"Signup Visual\"\r\n            className=\"w-full h-full object-cover rounded-t-lg md:rounded-l-lg md:rounded-tr-none\"\r\n          />\r\n        </div>\r\n\r\n        {/* Right Form */}\r\n        <div className=\"w-full md:w-1/2 p-8\">\r\n          <h2 className=\"text-2xl font-semibold mb-6\">Create Your Account</h2>\r\n\r\n          {/* Names */}\r\n          <div className=\"flex flex-col md:flex-row gap-4 mb-4\">\r\n            <div className=\"w-full\">\r\n              <label className=\"block text-sm font-medium mb-1\">First Name*</label>\r\n              <input\r\n                type=\"text\"\r\n                placeholder=\"e.g John\"\r\n                className=\"w-full border border-gray-300 rounded px-3 py-2\"\r\n              />\r\n            </div>\r\n            <div className=\"w-full\">\r\n              <label className=\"block text-sm font-medium mb-1\">Last Name*</label>\r\n              <input\r\n                type=\"text\"\r\n                placeholder=\"e.g Doe\"\r\n                className=\"w-full border border-gray-300 rounded px-3 py-2\"\r\n              />\r\n            </div>\r\n          </div>\r\n\r\n          {/* Contact Info */}\r\n          <div className=\"flex flex-col md:flex-row gap-4 mb-4\">\r\n            <div className=\"w-full\">\r\n              <label className=\"block text-sm font-medium mb-1\">Email*</label>\r\n              <input\r\n                type=\"email\"\r\n                placeholder=\"e.g john@gmail.com\"\r\n                className=\"w-full border border-gray-300 rounded px-3 py-2\"\r\n              />\r\n            </div>\r\n            <div className=\"w-full\">\r\n              <label className=\"block text-sm font-medium mb-1\">Phone No.*</label>\r\n              <input\r\n                type=\"tel\"\r\n                placeholder=\"e.g 072XXXXXXX\"\r\n                className=\"w-full border border-gray-300 rounded px-3 py-2\"\r\n              />\r\n            </div>\r\n          </div>\r\n\r\n          {/* Passwords */}\r\n          <div className=\"flex flex-col md:flex-row gap-4 mb-4\">\r\n            <div className=\"w-full\">\r\n              <label className=\"block text-sm font-medium mb-1\">Password*</label>\r\n              <input\r\n                type=\"password\"\r\n                placeholder=\"Create Password\"\r\n                className=\"w-full border border-gray-300 rounded px-3 py-2\"\r\n              />\r\n              <p className=\"text-xs text-gray-500 mt-1\">Must be 8 characters at least</p>\r\n            </div>\r\n            <div className=\"w-full\">\r\n              <label className=\"block text-sm font-medium mb-1\">Confirm Password*</label>\r\n              <input\r\n                type=\"password\"\r\n                placeholder=\"Re-enter Password\"\r\n                className=\"w-full border border-gray-300 rounded px-3 py-2\"\r\n              />\r\n            </div>\r\n          </div>\r\n\r\n          {/* Company Name */}\r\n          <div className=\"mb-6\">\r\n            <label className=\"block text-sm font-medium mb-1\">Company Name</label>\r\n            <input\r\n              type=\"text\"\r\n              placeholder=\"e.g West Brook\"\r\n              className=\"w-full border border-gray-300 rounded px-3 py-2\"\r\n            />\r\n          </div>\r\n\r\n          {/* Sign Up Button */}\r\n          <div className=\"mb-4\">\r\n            <button className=\"w-full bg-blue-600 text-white py-2 rounded hover:bg-blue-700 transition\">\r\n              SIGN UP\r\n 